{
  "name": "client",
  "proxy": "http://localhost:7000",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "sanitize.css": "^12.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "tsx",
      "js",
      "ts"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.tsx$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)sx"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
